{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", {
        "endOfLine": "auto"
      }]
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    }
}
